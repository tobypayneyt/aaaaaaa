{
	"compilerOptions": {
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ES2015",
		"moduleResolution": "Classic",
		"target": "es6",
		"jsx": "react",
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"outDir": "js",
	},
	"exclude": ["node_modules", "**/node_modules/*"]
}
